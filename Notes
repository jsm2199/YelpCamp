#sec27 MongoDB on c9

sudo apt-key adv --keyserver hkp://keyserver.ubuntu.com:80 --recv 2930ADAE8CAF5059EE73BB4B58712A2291FA4AD5
echo "deb [ arch=amd64 ] https://repo.mongodb.org/apt/ubuntu trusty/mongodb-org/3.6 multiverse" | sudo tee /etc/apt/sources.list.d/mongodb-org-3.6.list
sudo apt-get update
sudo apt-get install -y mongodb-org
# You should now have mongo 3.6.2 or newer, you can double check with 
mongo --version 

mkdir data
echo "mongod --dbpath=data --nojournal" > mongod
chmod a+x mongod

# if mongo.exe hangs dont use inside cygwin
https://stackoverflow.com/questions/19912435/mongodb-connecting-to-test-hanging-and-mongo-shell-not-starting?rq=1

